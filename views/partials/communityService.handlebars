{{!Event Partial Template}}
{{!Author: Adam Kerr, Keegan, Kenneth }}

<section class = "community-service-holder">
  <div id = "community-service-picture">
    <img src={{sourceCom}} />
  </div>
  <div id = "community-service-name" class="community-service-name-search">
    {{nameCom}}
  </div>
  <div id = "community-service-details">
    {{detailsCom}}
  </div>
  <div id = "community-service-description">
    {{descriptionCom}}
  </div>
  {{#unless faqtsCom}}
    <div class="going">Look whos going!</div>
  <div class="overall">
  <div class="rsvp-holder">
  <div id="community-service-rsvp" class = "community-service-rsvp-list{{@index}}">
    {{#each going}}
      {{> rsvp}}
    {{/each}}
  </div>
    </div>
    <div class="buttoninside">
    {{> button}}
    </div>
  {{/unless}}
  </div>
</section>